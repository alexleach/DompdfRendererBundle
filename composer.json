{
    "name": "alexleach/dompdf-renderer",
    "description": "A Kimai 2 plugin which renders PDFs using Dompdf",
    "homepage": "https://www.github.com/alexleach/kimai-dompdf-renderer",
    "type": "kimai-plugin",
    "version": "0.1",
    "license": "MIT",
    "keywords": [
        "kimai",
        "kimai-plugin",
        "pdf",
        "dompdf"
    ],
    "authors": [
        {
            "name": "Alex Leach",
            "homepage": "https://github.com/alexleach"
        }
    ],
    "autoload": {
        "psr-4": {
            "KimaiPlugin\\DompdfRendererBundle\\": ""
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": false,
            "symfony/flex": false,
            "symfony/runtime": false
        },
        "platform": {
            "php": "8.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "kimai": {
            "require": 20000,
            "name": "Alternative PDF Renderer",
            "comment": "might actually work on earlier versions, not tested..."
        }
    },
    "require": {
        "dompdf/dompdf": "^2.0"
    },
    "require-dev": {
        "dompdf/dompdf": "^2.0"
    }
}
